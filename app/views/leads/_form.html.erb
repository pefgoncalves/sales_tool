<%= simple_form_for(@lead) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :contact %>
    <%= f.input :company %>
    <%= f.input :status %>
    <%= f.input :email %>
    <%= f.input :phones %>
    <%= f.association :user %>
    <%= f.association :flow %>
    <%= f.input :extra %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
